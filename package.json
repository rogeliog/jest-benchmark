{
  "version": "0.3.0",
  "main": "build",
  "author": "Rogelio Guzman <rogelioguzmanh@gmail.com>",
  "contributors": [
    "Sébastien Dufour-Beauséjour <sebastien.dufourbeausejour@gmail.com>"
  ],
  "license": "MIT",
  "files": [
    "bin/",
    "build/"
  ],
  "bin": "bin/jest-benchmark.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint . --config ./.eslintrc.json --no-eslintrc",
    "watch": "babel src -w --ignore **/*.test.js -d build",
    "prebuild": "rimraf build/",
    "build": "babel src --ignore **/*.test.js -d build",
    "prepublish": "yarn build",
    "format": "prettier --write \"**/*.js\""
  },
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "eslint": "^7.13.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.6.3",
    "jest-watch-typeahead": "^0.6.1",
    "prettier": "^2.1.2"
  },
  "prettier": {
    "proseWrap": "never",
    "singleQuote": true,
    "trailingComma": "all"
  },
  "name": "jest-benchmarks",
  "description": "Benchmark Jest test runs",
  "repository": {
    "type": "git",
    "url": "https://github.com/rogeliog/jest-benchmark.git"
  },
  "homepage": "https://github.com/rogeliog/jest-benchmark",
  "dependencies": {
    "execa": "^1.0.0",
    "ora": "^3.0.0"
  }
}
